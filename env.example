# NIL Matchup Environment Configuration
# Copy this file to .env and fill in your actual values

# Application
REACT_APP_NAME=NIL Matchup
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here
REACT_APP_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
REACT_APP_STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
REACT_APP_STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Security
REACT_APP_JWT_SECRET=your_jwt_secret_here
REACT_APP_ENCRYPTION_KEY=your_encryption_key_here
REACT_APP_SESSION_SECRET=your_session_secret_here

# Database
REACT_APP_DATABASE_URL=your_database_url_here
REACT_APP_DATABASE_PASSWORD=your_database_password_here

# Email Configuration
REACT_APP_EMAIL_SERVICE=gmail
REACT_APP_EMAIL_USER=your_email@gmail.com
REACT_APP_EMAIL_PASSWORD=your_email_password_here
REACT_APP_EMAIL_FROM=noreply@nilmatchup.com

# File Storage
REACT_APP_STORAGE_BUCKET=your_storage_bucket_here
REACT_APP_STORAGE_REGION=us-east-1

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token_here

# External APIs
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
REACT_APP_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Business Information
REACT_APP_BUSINESS_NAME=NIL Matchup LLC
REACT_APP_BUSINESS_EMAIL=contact@nilmatchup.com
REACT_APP_BUSINESS_PHONE=+1-555-123-4567
REACT_APP_BUSINESS_ADDRESS=123 Business St, City, State 12345
REACT_APP_BUSINESS_WEBSITE=https://nilmatchup.com

# Legal
REACT_APP_LEGAL_EMAIL=legal@nilmatchup.com
REACT_APP_PRIVACY_EMAIL=privacy@nilmatchup.com
REACT_APP_SUPPORT_EMAIL=support@nilmatchup.com

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PAYMENTS=true
REACT_APP_ENABLE_MESSAGING=true
REACT_APP_ENABLE_DOCUMENT_SIGNING=true

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
REACT_APP_API_TIMEOUT=30000

# Production
REACT_APP_CDN_URL=https://cdn.nilmatchup.com
REACT_APP_API_BASE_URL=https://api.nilmatchup.com
REACT_APP_WEBHOOK_URL=https://api.nilmatchup.com/webhooks

# CORS
REACT_APP_ALLOWED_ORIGINS=http://localhost:3000,https://nilmatchup.com

# Rate Limiting
REACT_APP_RATE_LIMIT_WINDOW=900000
REACT_APP_RATE_LIMIT_MAX_REQUESTS=100

# Session
REACT_APP_SESSION_TIMEOUT=1800000
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_LOCKOUT_DURATION=900000 